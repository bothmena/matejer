<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <title>{% block title %}{% endblock %}</title>
    <meta content="width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no" name="viewport">
    <meta name="description" content="{% block meta_description %}{% endblock %}" />
    <link rel="icon" type="image/x-icon" href="https://matejer.storage.googleapis.com/images/favicon_32.ico" />
    <link rel="apple-touch-icon" href="https://matejer.storage.googleapis.com/apple-touch-icon-57.png" />
    <link rel="apple-touch-icon" sizes="72x72" href="https://matejer.storage.googleapis.com/apple-touch-icon-72.png" />
    <link rel="apple-touch-icon" sizes="114x114" href="https://matejer.storage.googleapis.com/apple-touch-icon-114.png" />
    {% stylesheets
        '@ABOMainBundle/Resources/public/css/matejer.min.css'
        'bundles/AdminLTE_2_3_0/plugins/iCheck/square/blue.css'
        '@ABOMainBundle/Resources/public/css/custom.min.css' filter='cssrewrite' %}
        <link rel="stylesheet" href="{{ asset_url }}" />
    {% endstylesheets %}
        {% javascripts
            '@ABOMainBundle/Resources/public/js/matejer.min.js'
            '@ABOMainBundle/Resources/public/js/matejer/matejer_v_1.0.js' %}
            <script src="{{ asset_url }}" defer></script>
        {% endjavascripts %}
    <!--[if lt IE 9]>
        <script src="https://oss.maxcdn.com/html5shiv/3.7.3/html5shiv.min.js"></script>
        <script src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js"></script>
    <![endif]-->
  </head>
    <body class="hold-transition register-page" ng-app="matejerApp" ng-controller="MatejerController" ng-init="init('blue','{{ app.request.getLocale() }}');{% block options %}{% endblock %}">
        {% block javascripts %}{% endblock %}
        <div class="register-box" style="margin-top: 45px;">
            {#{% if app.request.hasPreviousSession %}
                {% for type, flash_message in app.session.flashbag.all() %}
                    {% for msg in flash_message %}
                        <div class="alert alert-{{type}} alert-dismissable box-shadow">
                            <button type="button" class="close" data-dismiss="alert" aria-hidden="true">Ã—</button>
                            {{ msg }}
                        </div>
                    {% endfor %}
                {% endfor %}
            {% endif %}#}
            <div class="register-logo">
                <a href="{{ path('abo_main_home_home') }}"><img height="60" width="60" src="{{ get_image('images/logo.png', 'avatar', true) }}"/></a>
            </div>
            {% block form_box %}{% endblock %}
        </div>
        <script type="application/ld+json">
        {
            "@context" : "http://schema.org",
            "@type" : "WebSite",
            "name" : "{{ 'seo.website.name'|trans }}",
            "alternateName" : "{{ 'seo.website.alternateName'|trans }}",
            "url" : "{{ url('abo_main_home_home') }}",
            "genre": "{{ 'seo.website.genre'|trans }}",
            "image": "https://matejer.storage.googleapis.com/images/logo.png",
            "sameAs" : [
                "{{ fb_profile }}",
                "{{ gp_profile }}",
                "{{ tw_profile }}",
                "{{ in_profile }}",
                "{{ yt_profile }}"
            ],
            "potentialAction": {
                "@type": "SearchAction",
                "target": "{{ 'seo.website.potentialAction.target'|trans }}",
                "query-input": "required name=search_term_string"
            },
            "about": {
                "@type": "Thing",
                "name": "{{ 'seo.website.about.name'|trans }}",
                "description": "{{ 'seo.website.about.description'|trans }}",
                "url": "{{ url('abo_support_main_aboutus') }}"
            },
            "creator": {
                "@type": "Person",
                "name": "{{ 'seo.website.creator.first_name'|trans }}",
                "familyName": "{{ 'seo.website.creator.last_name'|trans }}",
                "birthDate": "{{ 'seo.website.creator.birthday'|trans }}",
                "birthPlace": {
                    "@type": "Place",
                    "address": {
                      "@type": "PostalAddress",
                      "addressCountry": "{{ 'seo.website.creator.birth_place.country'|trans }}",
                      "addressLocality": "{{ 'seo.website.creator.birth_place.locality'|trans }}"
                    }
                },
                "email": "{{ 'seo.website.creator.email'|trans }}",
                "telephone": "{{ 'seo.website.creator.telephone'|trans }}",
                "description": "{{ 'seo.website.creator.description'|trans }}",
                "address": {
                    "@type": "PostalAddress",
                    "addressCountry": "{{ 'seo.website.creator.address.country'|trans }}",
                    "addressLocality": "{{ 'seo.website.creator.address.locality'|trans }}",
                    "streetAddress": "{{ 'seo.website.creator.address.street'|trans }}",
                    "postalCode": "{{ 'seo.website.creator.address.postal_code'|trans }}"
                },
                "image": "https://matejer.storage.googleapis.com/images/website.creator.jpg"{#,
                "url": "https://www.matejer.com" creator url#}
            },
            "keywords": "{{ 'seo.website.keywords'|trans }}",
            "inLanguage": [{
                    "@type": "Language",
                    "name": "{{ 'seo.website.language.en'|trans }}"
                },{
                    "@type": "Language",
                    "name": "{{ 'seo.website.language.fr'|trans }}"
                },{
                    "@type": "Language",
                    "name": "{{ 'seo.website.language.ar'|trans }}"
                }]
        }
    </script>
        <script>
            (function(i,s,o,g,r,a,m){i['GoogleAnalyticsObject']=r;i[r]=i[r]||function(){
            (i[r].q=i[r].q||[]).push(arguments)},i[r].l=1*new Date();a=s.createElement(o),
            m=s.getElementsByTagName(o)[0];a.async=1;a.src=g;m.parentNode.insertBefore(a,m)
            })(window,document,'script','https://www.google-analytics.com/analytics.js','ga');
            ga('create', 'UA-78469348-1', 'auto');
            ga('send', 'pageview');
        </script>
    </body>
</html>
