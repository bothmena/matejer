<!DOCTYPE html>
<html>
    <head>
        <meta charset="utf-8">
        <meta http-equiv="X-UA-Compatible" content="IE=edge">
        <meta content="width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no" name="viewport">
        <title>{% block title %}{% endblock %}</title>
        <meta name="description" content="{% block meta_description %}{% endblock %}" />
        <link rel="icon" type="image/x-icon" href="https://matejer.storage.googleapis.com/images/favicon_32.ico" />
        <link rel="apple-touch-icon" href="https://matejer.storage.googleapis.com/apple-touch-icon-57.png" />
        <link rel="apple-touch-icon" sizes="72x72" href="https://matejer.storage.googleapis.com/apple-touch-icon-72.png" />
        <link rel="apple-touch-icon" sizes="114x114" href="https://matejer.storage.googleapis.com/apple-touch-icon-114.png" />
        {% block meta_tags %}{% endblock %}
        {% javascripts
            '@ABOMainBundle/Resources/public/js/matejer.min.js'
            '@ABOMainBundle/Resources/public/js/matejer/matejer_v_1.0.js' %}
            <script src="{{ asset_url }}"></script>
        {% endjavascripts %}
        {% block javascripts %}{% endblock %}
        {% stylesheets 
    	    '@ABOMainBundle/Resources/public/css/matejer-seller.min.css'
            '@ABOMainBundle/Resources/public/css/custom.css' filter='cssrewrite' %}
        	<link rel="stylesheet" href="{{ asset_url }}" />
        {% endstylesheets %}
        {% block stylesheets %}{% endblock %}
        <!--[if lt IE 9]>
            <script src="https://oss.maxcdn.com/html5shiv/3.7.3/html5shiv.min.js"></script>
            <script src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js"></script>
        <![endif]-->
    </head>
    <body class="hold-transition skin-red fixed sidebar-mini" ng-app="matejerApp" ng-controller="MatejerController" ng-init="init('red','{{ app.request.getLocale() }}');{% block options %}{% endblock %}">
        <!-- Site wrapper -->
        <div class="wrapper">
            <header class="main-header">
                    {% if page is not defined %}{% set page = '' %}{% endif %}
            {% include 'ABOShopBundle::dashHeader.html.twig' with {'user': app.user, 'page': page} %}
            </header>
            
            <aside class="main-sidebar">
                {% block sidebar %}{% endblock sidebar %}
            </aside>
            
            <div class="content-wrapper">
            	<section class="content-header">
                    {% block content_header %}{% endblock %}
                </section>
                <section class="content">
                    {% block content %}{% endblock %}
                </section>
            </div>
            
            <footer class="main-footer">
            	{% include "::footer.html.twig" %}
            </footer>
        </div>
        {% block page_seo %}{% endblock %}
        <script>
            (function(i,s,o,g,r,a,m){i['GoogleAnalyticsObject']=r;i[r]=i[r]||function(){
            (i[r].q=i[r].q||[]).push(arguments)},i[r].l=1*new Date();a=s.createElement(o),
            m=s.getElementsByTagName(o)[0];a.async=1;a.src=g;m.parentNode.insertBefore(a,m)
            })(window,document,'script','https://www.google-analytics.com/analytics.js','ga');
            ga('create', 'UA-78469348-1', 'auto');
            ga('send', 'pageview');
        </script>
    </body>
</html>

